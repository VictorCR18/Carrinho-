generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
}

model Produto {
  id         Int     @id @default(autoincrement())
  nome       String
  categoria  String
  preco      Float
  descricao  String?
  imagem     String?
  quantidade Int     @default(0)
}
